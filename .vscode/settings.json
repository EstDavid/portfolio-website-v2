{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#e6ac0d",
    "activityBar.background": "#c49923",
    "activityBar.foreground": "#504d4d",
    "activityBar.inactiveForeground": "#47454599",
    "activityBarBadge.background": "#ee90bb",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1f6fd0",
    "statusBar.background": "#e6ac0d",
    "statusBar.foreground": "#333131",
    "statusBarItem.hoverBackground": "#1f6fd0",
    "statusBarItem.remoteBackground": "#1857a4",
    "statusBarItem.remoteForeground": "#353333",
    "titleBar.activeBackground": "#e6ac0d",
    "titleBar.activeForeground": "#252323",
    "titleBar.inactiveBackground": "#1857a499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "editor.formatOnSave": true,
  "eslint.useFlatConfig": false,
  "javascript.format.indentSwitchCase": false,
  "typescript.format.indentSwitchCase": false,
  "javascript.format.insertSpaceBeforeFunctionParenthesis": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true
}
